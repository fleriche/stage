
all : rapport cleanrep

rapport :
	rubber -d rapport.tex
	mv rapport.pdf pdf

# Clean commands
clean : cleanpdf cleanrep 

cleanpdf :
	rm -f *.pdf

# Nettoyage du r√©pertoire
cleanrep:
	rm -f *.aux *.cb *.cb2 *.log *.toc *.out *.lot *.lof *.maf *.mtc *.glsdefs *.ist *.ilg *.ind *.mtc0 *.synctex.gz
